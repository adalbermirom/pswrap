#!/usr/local/bin/prisma

local funcao teste_versao(n_versao)
    local Major, Minor, Release = string.separe(_VERSAO,'(%d+)%.(%d+)%.(%d+)');
    local Major, Minor, Release = convnumero(Major), convnumero(Minor),convnumero(Release);
    se Release < n_versao entao
        imprima("\n\tEste programa requer Prisma 1.0." .. convstring(n_versao) .. " ou superior");
        imprima("\tSua versão atual: Prisma " .. _VERSAO);
        sis.saia(1);
    fim
fim

teste_versao(107);


//fazendo o programa ficar mais eficiente com variáveis locais.
local escreva_footer, escreva_menu_funcoes, escreva_top, escreva_header, html_escape, html_escape_dec, procure_tag, mk_logo, mk_fonte, mk_autor, mk_veja, mk_header_tag, insira_linhas, verifique_header, mk_html_tag, processe_funcao, processe_var, processe, processe_fonte, crie_id, obt_atual_id, obt_nome_var, obt_nome_funcao, analise, grave_arquivo, leia_arquivo;

local string = string; 
local es = es;
local tabela = tabela;

local estrito = inclua'estrito';
local hl = inclua'colorize';


local funcao mostre_info()
    imprima '\t:::::::::::::::::::::::::::::::::::::::::::'
    imprima('\t: Minidoc-0.1 linguagem Prisma - nov/2023 :');
    imprima('\t:.........................................:');
fim

local funcao use()
    mostre_info();
    imprima('\n\tUse: ' .. args[0] .. ' entrada.prisma\n');
    imprima('\n Cuidado! Se houver uma pasta doc/ ela será mesclada com os novos documentos!');
fim

local dir = inclua'dir';

estrito.modo_global();//nesse modo as variáveis globais não podem mudar de tipo. E variáveis não declaradas causam erro.

local _id_cont=0;
funcao crie_id()
    _id_cont = _id_cont + 1;
    retorne 'id_' .. string.formate('%06X', _id_cont);
fim

local _id_menu_principal = crie_id(); //id do menu principal

funcao principal(args)

    se nao args[1] entao //caso não haja argumento, imprima a mensagem de ajuda e saia.
        use();
        sis.saia(1);
    fim
    
    //pasta doc é onde o manual html gerado será salvo.
    se nao dir.dir_existe('doc') entao //verifica se a pasta doc existe, se não existir ela é criada.
        local ret, err = dir.criedir('doc');
        se nao ret entao erro(err); fim
    fim
    
    local file = args[1]; //o primeiro e único arg é o nome do arquivo de entrada a ser processado.
    local p, a, ex = sis.nome_arquivo(file);
    p = p ou '';
    local pasta = args[2] ou 'doc/';
    se pasta:corte(-1,-1) <> '/' entao pasta = pasta..'/'; fim
    local file_out = p..pasta..a..'.html'; //para o nome de saída apenas acrescento a extensão html.

    local txt = leia_arquivo(file); //lendo o arquivo para a var txt;
    //imprima(txt);
    
    local tab, err = processe_fonte(txt); //função central que ira procurar por documentação
                                          //em caso de erro, é retornado falos + msg de erro.
                                          
    
    
    se nao tab entao //caso seja retornado falso é feito o tratamento de erro e o programa é encerrado.
        es.erro:escreva('Arquivo: ' .. file .. ' --> ' .. err .. '\n\n');
        sis.saia(1);
    fim
    
    link_nomes(tab);
         
    local a,err = es.abra(file_out, 'eb'); //tudo deu certo então vamos abrir o arq. de saída para gravar o resultado.
    se nao a entao erro(err); fim //em caso de erro é finalizado...
    escreva_top(a, tab);  //escrevemos o topo do html (header);
    crie_menu_flutuante(a,tab); //criamos um menu flutuante apos abertura do body.
    escreva_header(a, tab);  //escrevemos o header(cabeçalho) da documentação.
    escreva_funcoes(a, tab); //escrevemos o menu links com a lista de funções documentadas. 
    escreva_vars(a, tab);  // o mesmo com as variáveis se houver...
    escreva_sections(a, tab);//escreve as seções se houver...
    escreva_footer(a,tab); // escrevemos o footer(rodapé) do html 
    a:escreva(html_end); // finalizamos, fechamento de tags body e html.
    a:feche();
    
    grave_arquivo('doc/tema_padrao.css', html_padrao_css);  //grava o arquivo css
    grave_arquivo('doc/tema_dark.css', html_dark_css);
    grave_arquivo('doc/light_simple.css', html_light_css);
    grave_arquivo('doc/index.js', html_index_js); //grava o arquivo js
    grave_arquivo('doc/temas_itens.json', temas_json);
    grave_arquivo('doc/prisma.png', prisma_logo:dec_base64()); // grava a imagem prisma que está no fim deste programa em base 64.
    mostre_info(); //por fim mostra a informação do programa minidoc
    poe'\tProcesso terminado! Veja a pasta doc/'; //e imprime a frase final!
    retorne 0;
fim

função string_links(tlink)
    local s = '';
    para k, v em ipares(tlink) início
        s = s .. tlink[v] .. '\n';
    fim
    retorne s;
fim

função procure_link_troque(t, s)
    T(t); S(s);
    local ts = s:analise('\n', 1);
    
    local tlen = #t;
    para i=1, tlen início
        //imprima(t[i]);
        t[i] = t[i]:troque('[%w_]+', funcao(word)
            se #word < 2 então retorne word; fim
            local proc_link, tmp;
            para i,v em ipares(ts) início
                v:separe('<a.*>(.*)</a>'):troque('[%w_]+', funcao(vw) 
                    se word == vw então
                        proc_link = vw;
                    fim
                fim);
                //<a href="#id_000002">}Array; //</a>
                se proc_link então tmp = v; quebre; fim //busca encontrada então quebramos.
            fim
            local compare = '<h2 class="subtitulo-doc">';
            se proc_link e t[i]:corte(1, #compare) <> compare então 
                //retorne (tmp:corte( 1, 21) .. word ..'</a>');
                //imprima(t[i]);
            fim
        fim) //fim t[i]:troque(...)
    fim
   // imprima(s);
fim

//procurar ${} em: tab[idx], tab.vars[idx], tab.section[idx]
// se houver um link correspondente em: tab.funcs[idx], tab.vars.menu[idx],
//tab.section.menu[idx]
função link_nomes(tab)
    local s = string_links(tab.funcs) ..
              string_links(tab.vars.menu) ..
              string_links(tab.section.menu);
              
    procure_link_troque(tab, s);
    procure_link_troque(tab.vars, s);
    procure_link_troque(tab.section, s);

fim

local funcao crie_select(label, id, events, tab_itens)
    S(label,id,events);
    T(tab_itens);
/*  <label for="cars">Choose a car:</label>
  <select name="cars" id="cars">
    <option value="volvo">Volvo</option>
    <option value="saab">Saab</option>
    <option value="opel">Opel</option>
    <option value="audi">Audi</option>
  </select>
*/
    local id = id; //id para combo box muda tema.
    local s = '\n<label class="titulo" for="'..id..'">' .. label .. '</label>\n';
    s = s .. '<select class="titulo" name="' .. id .. '" id="' ..id .. '" ' .. events .. ' >\n';
    para i, v em ipares(tab_itens) inicio
        s = s .. '<option value="'.. v.val .. '">'..v.txt..'</option>\n';
    fim
    s = s .. '</select>\n';
    retorne s, id;
fim


//cria um menu flutuante do lado direito superior da tela.
funcao crie_menu_flutuante(a,tab)
   V('arquivo',a); T(tab);
/*
  <nav class="floating-menu">
    <button onclick="open_menu(this, 'menu_funcoes')">&#x2630 Menu</button>
    <span>Funções</span>
    <div id="menu_funcoes" hidden>    
    <h2>Floating Menu</h2>
    <a href="/css/">CSS</a>
    <a href="/html/">HTML</a>
    <a href="/coldfusion/">ColdFusion</a>
    <a href="/database/">Database</a>
    </div>
    </nav>
*/

  se nao tab.funcs[1] e nao tab.vars.menu[1] e nao tab.section.menu[1] entao retorne; fim
  
  local id_nav_menu_flutuante = crie_id();
  local id_div_menu_flutuante = crie_id();
  a:escreva('<nav id="' .. id_nav_menu_flutuante .. '"  class="floating-menu">\n');
  a:escreva('<div class="menu-button" title="Ocultar painel lateral" onclick="show_menu_flutuante(this, \''.. id_div_menu_flutuante ..'\', \'' .. id_nav_menu_flutuante ..'\')">&#x2630;</div>\n');
  //a:escreva('<button onclick="show_menu_flutuante(this, \''.. id_div_menu_flutuante ..'\', \'' .. id_nav_menu_flutuante ..'\')">&#x2630;</button>');
  //a:escreva('<span> Menu</span>\n');
  a:escreva('<div class="com-rolagem" id="' .. id_div_menu_flutuante .. '" hidden>\n');
  
  se tab.funcs[1] entao
      a:escreva('<div class="caixa ">');
      a:escreva('<h2>Funções:</h2>\n');
      tabela.ordene(tab.funcs);
      para i, v em ipares(tab.funcs) inicio
          a:escreva(tab.funcs[v],'\n'); 
      fim
      a:escreva('\n</div>\n');
  fim
  se tab.vars.menu[1] entao
      a:escreva('<div class="caixa ">');
      a:escreva('<h2>Variáveis:</h2>\n');
      tabela.ordene(tab.vars.menu);
      para i, v em ipares(tab.vars.menu) inicio
          a:escreva( tab.vars.menu[v],'\n');  
      fim
      a:escreva('</div>\n\n');
  fim
  
  se tab.section.menu[1] entao
      a:escreva('<div class="caixa com-borda">');
      a:escreva('<h2>Seções:</h2>\n');
      tabela.ordene(tab.section.menu);
      para i, v em ipares(tab.section.menu) inicio
          a:escreva( tab.section.menu[v],'\n');  
      fim
      a:escreva('</div>\n\n');
  fim
  
  a:escreva('</div>\n</nav>\n');  
fim


//cria o rodapé
funcao escreva_footer(a,tab)
    V('arquivo',a); T(tab);
    local foot = '<div class="footer"><img src="prisma.png" alt="Prisma logo" width="50"><br>Feito com minidoc-0.1\n<a href="https://linguagemprisma.br4.biz/blog">Linguagem Prisma</a></div>\n';
    foot = tab.header.raw_footer ou foot;
    a:escreva(foot);
fim

//cria o menu com os links da funções documentadas.
funcao escreva_menu_funcoes(a,tab)
    V('arquivo',a); T(tab);
    se nao tab.funcs[1] entao retorne; fim
    a:escreva('<div>');
    a:escreva('<h2 >Funções:</h2>\n<ol>\n');
    para i, v em ipares(tab.funcs) inicio
        a:escreva('<li>', tab.funcs[v], '</li>\n'); 
    fim
    a:escreva('</ol>\n</div>\n\n');
fim


//cria o menu com os links das variáveis documentadas.
funcao escreva_menu_vars(a,tab)
    V('arquivo',a); T(tab);
    se nao tab.vars.menu[1] entao retorne; fim
    a:escreva('<div>');
    a:escreva('<h2>Variáveis:</h2>\n<ol>\n');
    para i, v em ipares(tab.vars.menu) inicio
        a:escreva( '<li>',tab.vars.menu[v],'</li>\n' ); 
    fim
    a:escreva('</ol>\n</div>\n\n');
fim


//cria o menu com os links das seções documentadas.
funcao escreva_menu_sections(a,tab)
    V('arquivo',a); T(tab);
    se nao tab.section.menu[1] entao retorne; fim
    a:escreva('<div>');
    a:escreva('<h2>Seções:</h2>\n<ol>\n');
    para i, v em ipares(tab.section.menu) inicio
        a:escreva( '<li>', tab.section.menu[v], '</li>\n' ); 
    fim
    a:escreva('</ol>\n</div>\n\n');
fim


//Escreve todas as funções documentadas.
funcao escreva_funcoes(a,tab)
    V('arquivo',a); T(tab);
    se nao tab[1] entao retorne fim;
    a:escreva('<h1>Funções:</h1>\n');   
    para i,v em ipares(tab) inicio
       a:escreva(v);
    fim
fim


//escreve todas as variáveis documentadas
funcao escreva_vars(a,tab)
    V('arquivo',a); T(tab);
    se nao tab.vars[1] entao retorne fim;
    a:escreva('<h1>Variáveis</h1>\n');
    para i,v em ipares(tab.vars) inicio
        a:escreva(v);
    fim
fim


//escreve todas as seções documentadas
funcao escreva_sections(a,tab)
    V('arquivo',a); T(tab);
    se nao tab.section[1] entao retorne fim;
    a:escreva('<h1>Seções:</h1>\n');
    para i,v em ipares(tab.section) inicio
        a:escreva(v);
    fim
fim

//escreve o início do html <html> <header> etc.
funcao escreva_top(a, tab)
    V('arquivo',a); T(tab);
    local html_top = html_top:troque('${page_title}', tab.header.file);
    a:escreva(html_top);
fim


//escreve o 'header' da 'documentação' (não do html);
funcao escreva_header(a, tab)
    V('arquivo',a); T(tab);
    //a:escreva('<a id="' .. _id_menu_principal .. '" hidden>...</a>');
                                      //crie_select('Label', 'id', 'atribs ou events', tab_itens);
    local txt_select_theme, select_id = crie_select('Tema:', 'combo_tema', 'title="Mude o tema html aqui!" onchange="mude_tema(this);"',
    {
	    {val='tema_padrao.css', txt = 'Padrão'};
    })
    
    //imprima('---------> select id:', select_id);
    a:escreva('<div>'..txt_select_theme .. '</div>\n');
    
    //document.getElementById('tema_css').href = document.getElementById('id_00002D').value;
    
    a:escreva(tab.header.logo ou '');
    a:escreva(tab.header.fonte);
    a:escreva(tab.header.autor ou '');
    a:escreva(tab.header.data ou '');
    a:escreva(tab.header.versao ou '');
    local id_resumo = crie_id();
    se tipo(tab.header.raw_resumo)=='string' entao
        a:escreva('<br>\n<div>\n'.. html_escape(tab.header.raw_resumo) ..'\n<a href="#' .. id_resumo .. '"> mais...</a></div>\n');
    fim
    
    se tab.funcs[1] ou tab.vars.menu[1] ou tab.section.menu[1] entao
        a:escreva('<div class="caixa com-borda">');
          tabela.ordene(tab.funcs);
          escreva_menu_funcoes(a,tab);
          tabela.ordene(tab.vars.menu);
          escreva_menu_vars(a,tab);
          tabela.ordene(tab.section.menu);
          escreva_menu_sections(a,tab);
        a:escreva('</div><br>\n');//fim caixa com borda menu funções e vars..
    fim //fim se tab.funcs ou ...
    
    se tab.header.raw_resumo entao //só aparece o resumo se tiver.
       a:escreva('\n<h2 id="', id_resumo, '">', tab.header.raw_fonte, '</h2>');
        a:escreva('<div>\n', (tab.header.raw_resumo ou ''), '\n</div>\n');
    fim
      para i,v em ipares(tab.header) inicio
          a:escreva(v);
      fim 
    //a:escreva('</div>');  
fim

//faz o escape dos caracteres html importantes para serem exibidos e não interpretados.
funcao html_escape(txt)
    S(txt);
    local tab = {
		['<'] = '&lt;';
        ['>'] = '&gt;';
        ["'"] = '&#39;';
        ['"'] = '&quot;';
        ['&'] = '&amp;';
	}
    retorne (txt:troque('[&<>\'"]', tab));
fim



//decodifica html escapado, revertendo ao normal ( ex.: '&lt;' => '<' )
funcao html_escape_dec(txt)
    S(txt);
    local tab = {
		['&lt;'] = '<';
        ['&gt;'] = '>';
        ['&#39;'] = "'";
        ['&quot;'] = '"';
        ['&amp;'] = '&';
		
	}
   retorne txt:troque('(&.-;)', tab);
fim



/**
   \fn localize_tag
   \brief Localiza a primeira tag se houver e retorna o indice inicial, indice final, tag, texto depois da tag até o fim da linha
   \param s(string) a string onde será feita a busca pela tag;
   \returns ini(numero ou falso) - índice inicial da string tag se encontra, caso contrário falso; 
   \returns fini(numero ou nulo) - índice final da tag ou nulo;
   \returns tag(string) - a tag encontrada.
   \returns txt(string) - o texto que vai desde o espaço após a tag até o final da string.
   
   <p><b>Obs.</b> é necessário deixar um espaço após a tag, ex.:  \\fonte nome_font.pris.</p>
   <p>Para citar uma tag dentro da documentação como feito acima, é necessário o escape, use duas barras seguidas e a tag será ignorada</p>
 */


funcao localize_tag(s)
    S(s);
    local ini, fini;
    local val, ret_tag;
    local i, j;
    i = 1;
    enquanto i <= #s inicio //enquanto 1
        //imprima(s[i]);
        se s[i]=='\\' ou s[i]=='@' entao //barra invertida encontrada ou arroba
            i = i + 1; //passa para o próximo;
            se s[i]=='\\' entao //outra barra significa escape, logo é ignorada...
                i = i+1;
                vapara continue;
            senao //econtramos uma possível tag:
                ini = i;
                enquanto i <= #s inicio
                    //imprima(s[i]);
                    se s[i]==' ' ou s[i] == '\n' ou i>=#s entao  
                        se i>=#s entao
                            fini = i;
                        senao                      
                            fini = i-1;
                        fim
                        //imprimaf('%q\n',s:corte(ini-1,fini) );
                        retorne ini-1, fini, s:corte(ini-1, fini):apare(), s:corte(fini+2, -1);
                    senaose s[i]=='\\' entao 
                        retorne falso, 'Tag mal formada, verifique se há o espaço depois da tag!', '';
                    fim
                    //imprimaf(s[i]);
                    i = i + 1;
                fim
            fim
            
        fim
        i = i + 1;
        ::continue::
    fim //enquanto 1
    retorne falso;        
fim

local funcao procure_tag(s, ...)
     S(s);
     local tgs = {...};
     local ini, fini, tag, txt = localize_tag(s); //procura por uma tag, a primeira e retorna
     se ini entao
         para i=1, #tgs inicio
             se tgs[i]:maiuscula() == tag:maiuscula() entao
                 retorne ini, fini, tag, txt;
             fim
         fim
     fim
     //nada foi encontrado, logo o retorno é falso + mensagem de erro.
     retorne falso, fini;
fim

//é necessário fazer o escape de strings de busca em prisma.
local funcao escape_padrao(s)
    retorne s:troque('[%+%%%-%.%*%[%]%(%)%^%$]',
    funcao(a)
        retorne '%'..a;
    fim);
fim
local funcao busque_lin(txt,n)
    local ini, fin, tmp = 1, 0;
    para i=1, n inicio
        ini, fin = txt:procure('\n',fin+1); 
        se nao ini entao quebre; fim
    fim
    retorne fin;
fim
local funcao lin_preprocessor_erro(txt, src)
    local busca = escape_padrao(src);
    local ini, fini = string.procure(txt, busca);    
    se ini entao
        txt = txt:corte(1, ini);
        local txt, n = txt:troque('\n','\n');//troca os '\n'por'\n' e retorna a quantidade de trocas feitas
        //imprima('====>', txt[n]);
        local ini_lin = busque_lin(txt, n);
        se txt:corte(ini_lin+1,#txt):apare():corte(1,3)=='\\--' entao            
            retorne nulo, 'coment'; //verifica se a linha começa com \-- se sim é comentário de minidoc.
        fim
        retorne n;
    fim    
fim

local funcao preprocessor(txt)
    local orig_txt=txt;
    local txt = txt:troque('<%%(.-)%%>', funcao(s)
    estrito.desative();
        local f, err = carregue(s);
        se nao f entao 
            local nerr = convnumero(err:separe(':(%d+):'));
            local cont, srr = lin_preprocessor_erro(orig_txt, s);
            se nao cont e srr=='coment' entao retorne ''; fim //retorna pois é comentário doc
            se nerr entao cont = cont + nerr; fim
            imprima('------------------------------------------------\n'..
                    '\t[lin: '..cont.. '] Erro em tempo de carregamento do preprocessador:\n\n'..err); 
             imprima('\n*Veja o trecho:\n\n', s,'\n');
             estrito.modo_global(); 
             retorne '';//troca por string vazia em caso de erro.
        senao
            local a, ret = pchame(f);
            se nao a entao
                local cont, srr = lin_preprocessor_erro(orig_txt, s)
                se nao cont e srr=='coment' entao retorne ''; fim //retorna pois é comentário doc
                local nerr = convnumero(ret:separe(':(%d+):'));
                se nerr entao cont = cont + nerr; fim
                imprima('------------------------------------------------\n'..
                    '\t[lin: '..cont.. '] Erro em tempo de carregamento do preprocessador:\n\n'..ret); 
                imprima('\n*Veja o trecho:\n\n', s,'\n');
                estrito.modo_global(); 
                retorne '';//troca por string vazia em caso de erro.
            senao //tudo ok entao retorne o resultado da operação:
                ret = ret ou ' ';//se for nulo então retorna uma string vazia.
                estrito.modo_global(); 
                retorne convstring(ret);
            fim
        fim
    fim); 
    estrito.modo_global();   
    retorne txt;
fim

funcao mk_logo(t,i, larg)
    T(t); N(i,larg); 
    local img_txt, img_link;
    local fmt = '<img src="%s" alt="%s" width="%s">\n'
    local ini, fini, dtag, txt = procure_tag( t[i], '\\logo', '@logo');
    local primeiro_espaco = txt:apare():procure(' ');
    se primeiro_espaco entao
        img_link = txt:corte(1,primeiro_espaco-1);
        img_txt = txt:corte(primeiro_espaco+1, -1);
    senao
        img_link = txt;
        img_txt = '';
    fim
    t.tab.header.logo = string.formate(fmt, img_link:apare(), html_escape(img_txt), convstring(larg));
fim


//  \img 240 my/link/img.png descrição da minha imagem...
funcao mk_img(t,i, tout)
    T(t); N(i); 
    local img_larg, img_desc, img_link, primeiro_espaco, segundo_espaco;
     //<img src="img_girl.jpg" alt="Girl in a jacket" width="150" height=""> 
    local fmt = '<img class="com-borda" id="%s" src="%s" alt="%s" width="%s">\n' ..
[[
<script>
    let img_pjs = document.getElementById('%s');
    img_pjs.onclick = function(){
        img_pjs.width = (img_pjs.width==%s) ? 800 : %s;
    }
</script>
]]
    local ini, fini, dtag, txt = procure_tag( t[i], '\\img', '@img');
    primeiro_espaco = txt:apare():procure(' ');
    segundo_espaco = txt:apare():procure(' ', primeiro_espaco+1);//procura o próximo espaco se tiver
    se primeiro_espaco e segundo_espaco entao //foi encontrado um espaco
        img_larg = txt:corte(1, primeiro_espaco-1);
        img_link = txt:corte(primeiro_espaco+1, segundo_espaco-1);
        img_desc = txt:corte(segundo_espaco+1, -1);
    senao
        img_larg = '150';
        img_link = txt:apare();
        img_desc = '';
    fim
    local id = crie_id();
    tout:insira( string.formate(fmt, id, img_link, img_desc, img_larg, id, img_larg, img_larg) );
fim


funcao mk_fonte(t,i, append)
    T(t); N(i);  local append = append ou '';
    local ini, fini, dtag = procure_tag( t[i], '\\file', '\\fonte', '@file', '@fonte');
    local txt = t[i]:corte(fini+1, -1);
    t.tab.header.file = txt:apare();
    t.tab.header.raw_fonte = append .. txt:apare();
    t.tab.header.fonte = '<h1 id="' .. _id_menu_principal .. '">' .. html_escape(append .. txt) .. '</h1>\n';
fim


funcao mk_autor(t, i, append)
    T(t); N(i);  local append = append ou '';
    local ini, fini, dtag = procure_tag( t[i], '\\author', '\\autor', '@author', '@autor');
    local txt = t[i]:corte(fini+1, -1);
    t.tab.header.raw_autor = append .. txt:apare();
    t.tab.header.autor = '<h3>' .. html_escape(append .. txt) .. '</h3>\n';
fim

funcao mk_header_data(t, i, append)
    T(t); N(i); local append = append ou '';
    local ini, fini, dtag = procure_tag( t[i], '\\date', '\\data', '@date', '@data');
    local txt = t[i]:corte(fini+1, -1);
    t.tab.header.raw_data = append .. txt:apare();
    t.tab.header.data = '<h3>' .. html_escape(append .. txt) .. '</h3>\n';
fim

funcao mk_veja(t, i, tout)
    T(t); N(i);
    local ini, fini, dtag = procure_tag( t[i], '\\see', '\\veja', '@see', '@veja');
    local append = dtag:corte(2,2):maiuscula() .. dtag:corte(3,-1);
    local txt = t[i]:corte(fini+1, -1);
    tout:insira('<b>'..append..': </b><a href="' .. txt:apare() .. '">' .. html_escape(txt) .. '</a>\n');
fim


funcao mk_brief(t, i, tout)
    T(t, tout); N(i);
    local ini, fini, dtag, txt = procure_tag( t[i],  '\\brief', '@brief', '\\resumo', '@resumo','\\desc', '@desc');
    
    //imprima(txt);
    se t.tab.header == tout entao
        tout.raw_resumo = txt;
        tout.resumo = '<p>'.. html_escape(txt) .. '</p>\n';
    senao
        tout:insira('<p>'.. html_escape(txt) .. '</p>\n');
    fim
fim

funcao mk_header_tag(t,i, tag, atr, append, tabprocure)
    T(t); N(i);S(tag,atr,append); T(tabprocure);
    //t,i, 'h3', '', 'Data: ', {'\\date', '\\data', '@date', '@data'})
    local ini, fini, dtag = procure_tag( t[i], tabela.desempacote(tabprocure) );
    local txt = t[i]:corte(fini+1, -1);
    t.tab.header[#t.tab.header+1] = '<'..tag .. ' ' .. atr .. '>' .. html_escape(append .. txt).. '</'..tag.. '>\n';

fim


/**
 *   \fn procure_abertura_header
 *   \brief procura a abertura de comentário documentado /** ou /*@doc e depois a tag 'file' ou 'fonte'
 *   \param t(tabela) tabela com as linhas do arquivo aberto.
 *   \param i(numero) indice atual da linha 
 *   \returns (numero | falso, str_err) - retorna o índice da linha em caso de sucesso ou falso + mensagem de erro.
*/
funcao procure_abertura_header(t,i)
    T(t); N(i);
    local open_tag_doc=falso;
    local fecha_doc = '*/';
    
    para idx=i, #t inicio
        //imprima(idx, t[idx]);
        se t[idx]:procure('/%*%*') ou  t[idx]:procure('/%*@doc') entao
            open_tag_doc = verdadeiro;
        senaose t[idx]:procure('%-%-%[%[%[') ou  t[idx]:procure('%-%-%[%[@doc') entao
            open_tag_doc = verdadeiro;
            fecha_doc = '%]%]';
        fim
        se open_tag_doc e procure_tag( t[idx], '\\file', '\\fonte', '@file', '@fonte') entao
            imprima(t[idx]);
            retorne idx,'Início header encontrado com sucesso!', fecha_doc;
        fim
    fim
    retorne falso, 'Início header não encontrado!';
fim

local funcao mk_code(t,i,tout)
            local src = '';
            local ini, fini, ptag = procure_tag(t[i],'\\codigo','@codigo','\\code', '@code', '\\start_code', '@start_code', '\\inicio_codigo', '@inicio_codigo');
            local lang_info = t[i]:corte(fini+1, -1):apare();
            i = i + 2;//pula o \n
            ptag = ptag .. '--';
            enquanto i<#t inicio
                //imprima('--->>>>', t[i]);
                se procure_tag(t[i], ptag, '\\end_code', '@end_code', '\\fim_codigo', '@fim_codigo') entao
                    quebre;
                fim 
                src = src .. t[i];
                i = i + 1;
            fim
            
            local id = crie_id();
            //pre invisível contendo o conteúdo fonte exemplo limpo sem span para ser copiado.
            tout:insira('<div class="caixa com-borda"><div class="tool-button float-right-top" title="Copiar para área de transferência." onclick="copyText(\''.. id .. '\')">&#128203;</div>&nbsp;<i>' .. lang_info .. '</i>');
            tout:insira('<pre id="'..id..'" hidden>');
            //src = insira_linhas(src);
            //imprima(src);
            //leia();
            tout:insira(html_escape(src));
            tout:insira('</pre>');
            local colorized = hl.colorize_str(src, hl.lista_prisma);
           // poe'\n\n-----------------\n';
            //imprima(colorized);
            //leia();
            tout:insira(colorized);
            //t.tab:insira('</code></pre>\n</div>');
            tout:insira('\n</div>');
    retorne i;
fim

local funcao mk_bloco(t,i, tout)
//imprima'....................start cooddeeeee';
            local src = '';
            local ini, fini, ptag = procure_tag(t[i],'\\bloco','@bloco', '\\block', '@block', '\\start_block', '@start_block', '\\inicio_bloco', '@inicio_bloco');
            local bloco_info = t[i]:corte(fini+1, -1):apare();
            i = i + 2; //pula o \n
            ptag = ptag .. '--';
            enquanto i<#t inicio
                se procure_tag(t[i], ptag, '\\end_block', '@end_block', '\\fim_bloco', '@fim_bloco') entao
                    quebre;
                fim 
                src = src .. t[i];
                i = i + 1;
            fim
            local id = crie_id();
            //pre com o conteúdo já escapado em html:
            tout:insira('<div><i>' .. bloco_info.. '</i>\n');
            //tout:insira('<div class="tool-button float-right-top" title="Copiar para área de transferência." onclick="copyText(\''.. id .. '\')">&#128203;</div>&nbsp;<i>' .. bloco_info .. '</i>');
            tout:insira('<pre class="bloco com-rolagem" id="'..id..'">');            
            tout:insira('<code>');
            tout:insira(html_escape(src));
            tout:insira('</code></pre>\n</div>');
    retorne i;
fim

local funcao mk_footer(t,i, tout)
    T(t,tout); N(i); 
    local ini, fini, dtag = procure_tag( t[i], '\\footer', '\\rodape','\\rodapé', '@footer', '@rodape', '@rodapé');
    local txt = t[i]:corte(fini+1,-1);
    i = i + 1; //pula a tag encontrada;
    local dtag_end = dtag..'--';
    enquanto i<#t inicio
        se procure_tag(t[i], dtag_end, '\\end_footer', '@end_footer', '\\fim_rodape', '@fim_rodape','\\fim_rodapé', '@fim_rodapé') 
        entao
            quebre;
        fim 
        txt = txt .. t[i];
        i = i + 1;
    fim
   // txt = preprocessor(txt);
    t.tab.header.raw_footer =  '<footer class="footer">\n'.. txt .. '</footer>\n';
    retorne i;
fim

local funcao mk_obs(t,i,tout)
    local ini, fini, tag, txt = procure_tag(t[i], '\\obs','@obs');
    tout:insira( '<p><i><b>Obs.: </b>' .. html_escape(txt) .. '</i></p>' );
fim

local funcao mk_titulo(t,i, tout)//(t,i, tout); 
    local ini, fini, tag, txt =  procure_tag( t[i], '\\title', '@title' , '\\titulo', '@titulo');
    tout:insira('<h1>' .. html_escape(txt) .. '</h1>');
fim

local funcao mk_subtitulo(t,i, tout)//(t,i, tout); 
    local ini, fini, tag, txt =  procure_tag( t[i], '\\subtitle', '@subtitle' , '\\subtitulo', '@subtitulo');
    tout:insira('<h2>' .. html_escape(txt) .. '</h2>');
fim

local funcao mk_h (t, i, tout)
    local ini, fini, tag, txt = procure_tag( t[i],'\\h1', '\\h2', '\\h3', '\\h4', '\\h5', '\\h6',
                                                  '@h1', '@h2', '@h3', '@h4', '@h5', '@h6' );
    local s = '<'..tag:corte(2,3) .. '>' .. html_escape(txt) .. '</'..tag:corte(2,3) .. '>\n';
    tout:insira(s);
fim

//    \link http:\\meulink.com minha descrição aqui até o final da linha
local funcao mk_link(t,i, tout)//(t,i, tout); 
    local ini, fini, tag, txt =  procure_tag( t[i], '\\link', '@link');
    local primeiro_espaco = txt:apare():procure(' ');//procura o primeiro espaço após o link.
    local meu_link, txt_link;
    se nao primeiro_espaco entao
        meu_link = txt:apare(); txt_link = txt:apare();
    senao
        meu_link = txt:corte(1,primeiro_espaco-1);
        txt_link = txt:corte(primeiro_espaco+1, -1):apare();
    fim
        
    se txt_link=='' entao //caso não haja nada somente o link, então o próprio link é o texto;
        txt_link = meu_link;
    fim
    tout:insira('<a target="_blank" href = "' .. meu_link .. '">' .. html_escape(txt_link) .. '</a>');
fim

local funcao mk_parag(t,i,tout)
    local ini, fini, tag, txt =  procure_tag(t[i],'\\parag', '@parag', '\\inicio', '@inicio', '\\start', '@start', '\\inicio_parag', '@inicio_parag','\\start_parag', '@start_parag');
    txt = txt ou '';
    local i = i+1;
    
    enquanto i<=#t inicio
        se procure_tag( t[i], tag..'--', '\\fim', '@fim', '\\end', '@end', '\\fim_parag', '@fim_parag', '\\end_parag', '@end_parag') entao
            quebre;
        fim
        txt = txt .. t[i];
        i=i+1;            
    fim
    tout:insira('<p>\n'.. html_escape(txt) .. '\n</p>');
    retorne i;
fim

local funcao mk_list(t,i,tout)
    local ini, fini, tag, txt =  procure_tag(t[i],'\\lista','@lista','\\list', '@list', '\\start_list', '@start_list', '\\inicio_lista', '@inicio_lista');
    local i = i+1;
    tout:insira('<ul>\n');
    enquanto i<=#t inicio
        se procure_tag( t[i], tag..'--', '\\fim_lista', '@fim_lista', '\\end_list', '@end_list') entao
            quebre;
        fim
        se t[i]<>'\n' entao
            tout:insira('<li>' .. html_escape(t[i])..'</li>\n');
        fim
        i=i+1;            
    fim
    tout:insira('</ul>\n');
    retorne i;
fim

local funcao mk_caixa_doc(t,i,tout)
    local ini, fini, tag, txt =  procure_tag(t[i],'\\caixa_doc','@caixa_doc','\\doc_box','@doc_box');
    tout:insira('<section class="caixa-doc">\n');
    tout:insira('<span class="float-right-top" ><a href="#" title="Voltar ao menu funções.">Voltar ao início  </a></span>');
    tout:insira('<h2 class="subtitulo-doc">'.. html_escape(txt)  ..  '</h2>');
    tout:insira('<div class="corpo-doc">');
fim


//função para processar as tags comuns em header, fn, vars e seção
local funcao comum_process(t,i,tout)
    local i = i;
    se  procure_tag( t[i], '\\obs', '@obs')  entao
            mk_obs(t,i, tout);//(t,i, tout);  
        
        senaose procure_tag( t[i],  '\\brief', '@brief', '\\resumo', '@resumo','\\desc', '@desc') entao
            mk_brief(t,i,tout);
        senaose procure_tag(t[i],'\\caixa_doc','@caixa_doc','\\doc_box','@doc_box') entao
            mk_caixa_doc(t,i,tout);
            
        senaose procure_tag(t[i],'\\caixa_doc--','@caixa_doc--','\\doc_box--','@doc_box--') entao
            tout:insira('</div>\n</section>\n'); //para finalizar a caixa_doc 
            
        senaose procure_tag( t[i], '\\see', '\\veja', '@see', '@veja') entao                        
            mk_veja(t,i, tout);
        
        senaose  procure_tag( t[i], '\\title', '@title' , '\\titulo', '@titulo')  entao
            mk_titulo(t,i, tout);//(t,i, tout);   
        
        senaose procure_tag( t[i], '\\subtitle', '@subtitle' , '\\subtitulo', '@subtitulo') entao
            mk_subtitulo(t,i, tout)//(t,i, tout); 
        
        senaose procure_tag( t[i],'\\h1', '\\h2', '\\h3', '\\h4', '\\h5', '\\h6',
                                                  '@h1', '@h2', '@h3', '@h4', '@h5', '@h6' ) entao
           mk_h(t,i, tout);
        
        senaose procure_tag(t[i], '\\link', '@link') entao
            mk_link(t, i, tout); 
        
        senaose procure_tag(t[i], '\\img', '@img') entao
            mk_img(t, i, tout);
        
        senaose procure_tag(t[i],'\\lista', '@lista', '\\list', '@list', '\\start_list', '@start_list', '\\inicio_lista', '@inicio_lista') entao
            i = mk_list(t, i, tout);
       
       senaose procure_tag(t[i],'\\parag','@parag','\\inicio', '@inicio', '\\start', '@start', '\\inicio_parag', '@inicio_parag','\\start_parag', '@start_parag') entao
            i = mk_parag(t, i, tout);
            
        senaose procure_tag(t[i],'\\code', '@code', '\\codigo','@codigo', '\\start_code', '@start_code', '\\inicio_codigo', '@inicio_codigo') entao
            i = mk_code(t,i, tout);
            
           //bloco_html para o header...
        senaose procure_tag(t[i],'\\bloco', '@bloco','\\block', '@block', '\\start_block', '@start_block', '\\inicio_bloco', '@inicio_bloco')  entao
            i = mk_bloco(t,i, tout);
        senaose procure_tag( t[i], '\\footer', '\\rodape','\\rodapé', '@footer', '@rodape', '@rodapé') entao             
            i = mk_footer(t,i, tout);
    senaose t[i]:apare():corte(1,3)=='\\--' então //comentário doc até o fim da linha não sai na documentação  
            i = i + 1; //pula esta linha pois é um comentário:      
    senao  
            t[i] = t[i]:troque('\\\\','\\'); //caso tenha duas barras (escape de barra \\) é trocado por uma só.
            //t[i] = preprocessor(t[i]);
            tout:insira(t[i]); //aqui não é usada a função html_escape(); fica a cargo do programador
                                       //para produzir a saída '*/' use o código html '&#42;/'
    fim
        retorne i;
fim

local funcao verifique_header(t,i, fecha_doc)
    T(t); N(i);
    local i = 1;
    local i, err, fecha_doc = procure_abertura_header(t,i)
    //imprima('retorne abertura header teste', i,err);
    se nao i entao retorne falso, err; fim
    local header_ok = falso;
    t.tab.header = {desc=''};
    funcao t.tab.header:insira(txt)
        este[#este+1] = txt;
    fim
    enquanto i <= #t inicio
      // imprima(i);
        se nao t[i] ou i>=#t ou t[i]:procure(fecha_doc)entao
           //imprima('saindo....', i, t[i]);
            quebre;
        fim
        se procure_tag( t[i], '\\file', '\\fonte', '@file', '@fonte') entao
            header_ok = verdadeiro;  
            //imprima('achou---------------');        
            mk_fonte(t,i,'');
            
        senaose procure_tag( t[i], '\\author', '\\autor', '@author', '@autor') e header_ok entao                        
            mk_autor(t,i, '');

        senaose procure_tag( t[i], '\\date', '\\data', '@date', '@data') e header_ok entao  
            mk_header_data(t,i,'');                    
            //mk_header_tag(t,i, 'h3', '', '', {'\\date', '\\data', '@date', '@data'});
            

        senaose procure_tag( t[i], '\\version', '\\versao','\\versão', '@version', '@versao','@versão') entao              
            local ini, fini, tag, txt = procure_tag( t[i], '\\version', '\\versao','\\versão', '@version', '@versao','@versão');
            t.tab.header.versao = '<h3>'.. html_escape(txt) .. '</h3>';
        senaose  procure_tag( t[i], '\\logo', '@logo')  entao
            mk_logo(t,i, 150)
        
        senao 
            i = comum_process(t,i, t.tab.header);    //processamento comum para header, funçao, var e seção;                
        fim
        i = i + 1;
    fim
    
    se nao header_ok entao retorne falso, 'Tag header incompleta ou inexistente'; fim

    retorne i;
fim

local funcao mk_html_tag(t,i, tag, atr, append, tabprocure)
    T(t); N(i);S(tag,atr,append); T(tabprocure);
    //t,i, 'h3', '', 'Data: ', {'\\date', '\\data', '@date', '@data'})
    local ini, fini, dtag = procure_tag( t[i], tabela.desempacote(tabprocure) );
    local txt = t[i]:corte(fini+1, -1);
    t.tab[#t.tab+1] = '<'..tag .. ' ' .. atr .. '>' .. html_escape(append .. txt).. '</'..tag.. '>\n';
fim

local funcao mk_html_tag_var(t,i, tag, atr, append, tabprocure)
    T(t); N(i);S(tag,atr,append); T(tabprocure);
    //t,i, 'h3', '', 'Data: ', {'\\date', '\\data', '@date', '@data'})
    local ini, fini, dtag = procure_tag( t[i], tabela.desempacote(tabprocure) );
    local txt = t[i]:corte(fini+1, -1);
    t.tab.vars:insira('<'..tag .. ' ' .. atr .. '>' .. html_escape(append .. txt).. '</'..tag.. '>\n');
fim

local funcao pule_fecha_comentario(t,i)
    T(t); N(i);
    para idx = i, #t inicio
        se t[idx]:procure(t.fecha_doc) entao
            retorne idx+1;
        fim
    fim
    retorne i;
fim

local funcao processe_funcao(t,i, fecha_doc)
    T(t); N(i);
    local id = crie_id();
    local parametros = falso; //quando a primeira tag param ativar é inserida o H2 'Parâmetros:'
    local retornos = falso;
    local lin_fn, nome_fn = obt_nome_funcao(t,i, fecha_doc);
    
    enquanto i <= #t inicio
        //imprima(t[i]);
        se t[i]:procure(fecha_doc) entao 
            //t.tab.vars:insira('</div>\n');
            t.tab:insira('</div>\n');
            t.tab:insira('</section>\n'); 
            retorne i;             
        fim
        
        se procure_tag(t[i], '\\fn', '@fn', '\\funcao', '@funcao', '\\função', '@função') entao
            local ini, fini, dtag, val = procure_tag(t[i], '\\fn', '@fn', '\\funcao', '@funcao', '\\função', '@função');
            se nao nome_fn entao nome_fn = val; fim
            se nao lin_fn entao lin_fn = 'funcao ' .. nome_fn .. '()'; fim
            t.tab.funcs[nome_fn] = '<a href="#' .. id .. '">' .. nome_fn .. '</a>';
            t.tab.funcs:insira(nome_fn);
            t.tab:insira('<a id="'.. id ..'" hiden></a>\n');           
            t.tab:insira('<section class="caixa-doc">\n');
           // t.tab:insira('<span class="float-right-top" ><a href="#' .. _id_menu_principal .. '" title="Voltar ao menu funções.">Voltar ao início  </a></span>');
            t.tab:insira('<span class="float-right-top" ><a href="#" title="Voltar ao menu funções.">Voltar ao início  </a></span>');
           // t.tab:insira('<h2 class="titulo-doc">' .. nome_fn .. '()</h2>');
            t.tab:insira('<h2 class="subtitulo-doc">'.. lin_fn  ..  '</h2>');
            t.tab:insira('<div class="corpo-doc">');
               
        senaose procure_tag(t[i], '\\param', '@param') entao
            se nao parametros entao
                parametros = verdadeiro;
                t.tab:insira('<div class="titulo-params"><h3>Parâmetros:</h3></div>');
            fim
            
            t.tab:insira('<div class="params-doc">');
            mk_html_tag(t,i,'span', '', '' , {'\\param', '@param'});
            t.tab:insira('</div>');
            
       senaose procure_tag(t[i], '\\returns', '@returns','\\return', '@return', '\\retorno', '@retorno') entao
            se nao retornos entao
                retornos = verdadeiro;
                t.tab:insira('\n<div class="titulo-returns"><h3>Retornos:</h3></div>');
            fim
            
            t.tab:insira('<div class="returns-doc">');
            //class="params">
            //<tr><td class="paramname"
            mk_html_tag(t,i,'span', '', '    ' , {'\\returns', '@returns','\\return', '@return', '\\retorno', '@retorno'});
            t.tab:insira('</div>');
        senao
            i = comum_process(t,i,t.tab); //t.tab[#t.tab+1] = t[i];
        fim
        i = i + 1;
    fim
    retorne i;
fim

local funcao processe_var(t,i, fecha_doc)
    T(t); N(i);
        local id = crie_id();
    local parametros = falso; //quando a primeira tag param ativar é inserida o H2 'Parâmetros:'
    local retornos = falso;
    local lin_var, nome_var = obt_nome_var(t,i, fecha_doc);
    //imprima(lin_var);
    se nao lin_var entao
        retorne nome_var;
    fim
    enquanto i <= #t inicio
        //imprima(t[i]);
        se t[i]:procure(fecha_doc) entao 
           // imprima('saindo------------', t[i]);
            t.tab.vars:insira('</div>\n');
            t.tab.vars:insira('</section>\n'); 
            retorne i;             
        fim
        
       se procure_tag(t[i], '\\var', '@var', '\\variavel', '@variavel', '\\variável', '@variável') entao
            
            t.tab.vars:insira_menu( nome_var );
            t.tab.vars.menu[nome_var] = '<a href="#' .. id .. '">' .. nome_var .. '</a>';
            t.tab.vars:insira('<a id="'.. id ..'" hiden></a>\n');
            t.tab.vars:insira('<section class="caixa-doc">\n');
            //t.tab.vars:insira('<h2>' .. nome_var .. ' = </h2>');
            //t.tab.vars:insira('<span class="float-right-top" ><a href="#' .. _id_menu_principal .. '" title="Voltar ao menu funções.">Voltar ao início  </a></span>');
            t.tab.vars:insira('<span class="float-right-top" ><a href="#" title="Voltar ao menu funções.">Voltar ao início  </a></span>');
            t.tab.vars:insira('<h2 class="subtitulo-doc">' .. lin_var  ..  '</h2>');
            t.tab.vars:insira('<div class="corpo-doc">');   
            
        
        senao 
            i = comum_process(t, i, t.tab.vars); // t.tab.vars:insira(t[i]);
        fim
        i = i + 1;
    fim
    retorne i;
    
fim



local funcao processe_section(t,i,fecha_doc)
    T(t); N(i);
    local id = crie_id();
    enquanto i <= #t inicio
        //imprima(t[i]);
        //imprima(fecha_doc);
        se t[i]:procure(fecha_doc) entao 
            //imprima('saindo------------ de section', t[i]);
            t.tab.section:insira('</div>\n');
            t.tab.section:insira('</section>\n'); 
            retorne i;             
        fim
        
       se procure_tag(t[i], '\\seção', '\\section', '@seção', '@section')  entao
            local ini, fini, dtag, nome_section = procure_tag(t[i], '\\seção', '\\section', '@seção', '@section');
            t.tab.section:insira_menu( nome_section );
            t.tab.section.menu[nome_section] = '<a href="#' .. id .. '">' .. nome_section .. '</a>';
            t.tab.section:insira('<a id="'.. id ..'" hiden></a>\n');
            t.tab.section:insira('<section class="caixa-doc">\n');
            //t.tab.section:insira('<h2>' .. nome_var .. ' = </h2>');
           // t.tab.section:insira('<span class="float-right-top" ><a href="#' .. _id_menu_principal .. '" title="Voltar ao menu funções.">Voltar ao início  </a></span>');
           t.tab.section:insira('<span class="float-right-top" ><a href="#" title="Voltar ao menu funções.">Voltar ao início  </a></span>');
            t.tab.section:insira('<h2 class="subtitulo-doc">' .. nome_section  ..  '</h2>');
            t.tab.section:insira('<div class="corpo-doc">');   
            
        senao 
            i = comum_process(t, i, t.tab.section); // t.tab.section:insira(t[i]);
        fim
        i = i + 1;
    fim
    retorne i;
    
fim


local funcao processe(t,i, fecha_doc)
    T(t); N(i);
    enquanto i<=#t inicio
        se t[i]:procure(fecha_doc) entao
            retorne i + 1;
        fim
        se procure_tag(t[i], '\\fn', '@fn', '\\funcao', '@funcao', '\\função', '@função') entao
            i = processe_funcao(t,i, fecha_doc);
            retorne i;
        senaose  procure_tag(t[i], '\\var', '@var', '\\variavel', '@variavel', '\\variável', '@variável') entao
            i = processe_var(t,i, fecha_doc);
            retorne i;
        senaose procure_tag(t[i], '\\seção', '\\section', '@seção', '@section') entao
            i = processe_section(t,i, fecha_doc);
            retorne i;
        fim
        i = i + 1;
    fim
    retorne i;
fim

local função abre_doc(t,i)
       T(t); N(i);
       se t[i]:procure('/%*%*') ou t[i]:procure('/%*@doc') entao
          t.fecha_doc = '%*/';
          retorne '%*/';
        senaose t[i]:procure('%-%-%[%[%[') ou t[i]:procure('%-%-%[%[@doc') entao
            t.fecha_doc = '%]%]';
            retorne '%]%]';
        fim
fim

funcao processe_fonte(txt)
    S(txt);
    local t = analise(txt);
    local i = 1;
    t.tab = {};
    funcao t.tab:insira(txt)
        este[#este+1] = txt;
    fim
    
    t.tab.funcs = {}; //tabela para guardar os nomes das funções.
    funcao t.tab.funcs:insira(txt)
        este[#este+1] = txt;
    fim
    t.tab.vars = {}; //lugar onde serão guardadas as variáveis.
    t.tab.vars.menu={};
    funcao t.tab.vars:insira(txt)
        este[#este+1] = txt;
    fim
    funcao t.tab.vars:insira_menu(txt)
        este.menu[#este.menu+1] = txt;
    fim
    
    t.tab.section = {menu={}};
    funcao t.tab.section:insira(txt)
        este[#este+1] = txt;
    fim
    funcao t.tab.section:insira_menu(txt)
        este.menu[#este.menu+1]=txt;
    fim
    
    i = verifique_header(t, i);
    //imprima('verifique header ', i);
    se nao i  entao retorne falso, 'cabeçalho não encontrado'; fim
    
    
    
    enquanto i<=#t inicio
        local fecha_doc = abre_doc(t, i);
        se fecha_doc entao
            i = processe(t,i, fecha_doc);
        fim
        i = i+1;        
    fim    
    retorne t.tab;
fim



funcao obt_nome_var(t,i)
    T(t); N(i);
    local i = pule_fecha_comentario(t,i);
    local lin_var = '';
    local tmp_i = i;
    enquanto 1 inicio
        se t[i]:procure'/%*%*' ou  t[i]:procure'/%*@doc'  ou i>= #t entao
            se i >= #t entao
                erro('Variável não encontrada, final de arquivo, inicio doc: '.. convstring(tmp_i) );
            fim
            retorne falso, i-1;
        senaose t[i]:procure'=' entao
            lin_var = t[i];
            quebre;
        fim        
        i = i + 1;
    fim   // local var = ...
    local var_nome = lin_var:separe('local%s(.*)=')
    se nao var_nome entao 
        var_nome = lin_var:separe('(.*)=');
    fim
    retorne lin_var, var_nome;
fim


funcao obt_nome_funcao(t,i)
    T(t); N(i);
    local tmp_i = t[i];//a linha onde foi encontrada a tag função
    local i = pule_fecha_comentario(t,i); //pula todo o restante do comentário, evita interpretar nome de função no
                                          //bem no meio de um comentário.
    local lin_func = '';
    enquanto 1 inicio
        //imprima(t[i]);
        se abre_doc(t,i)  ou i>= #t entao
            se i >= #t entao
                erro('Função não encontrada, final de arquivo, inicio doc: '.. convstring(tmp_i) );
            fim
            retorne i-1;
        senaose t[i]:procure'funcao' ou t[i]:procure'função' entao
            lin_func = t[i];
            quebre;
        fim        
        i = i + 1;
    fim   // local funcao ola(a,b,c)
    local so_nome = lin_func:separe('funcao%s(.*)%(')
    se nao so_nome entao 
        //imprima('dentro');
        so_nome = lin_func:separe('local (.*)=') ou lin_func:separe('(%w+.*)=') ou lin_func:separe('[%s%w]+%s(.*)=') 
    fim
    retorne lin_func, so_nome;
fim



funcao analise(txt)
    S(txt);
    local t = string.analise(txt, '\n\r');
    se nao t entao retorne {txt}; fim
    retorne t;
fim


funcao grave_arquivo(file, txt)
    S(file);
    local a,err = es.abra(file, 'eb');
    se nao a entao erro(err); fim
    a:escreva(txt);
    a:feche();
fim

funcao leia_arquivo(file)
    S(file);
    local a,err = es.abra(file, 'lb');
    se nao a entao erro(err); fim
    local txt = a:leia('*t');
    txt = preprocessor(txt);
    //imprima(txt);
    a:feche();
    retorne txt;
fim


